<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tiger.consumer.dao.ConsumerTypeDao">
     <resultMap id="consumerType" type="com.tiger.consumer.entity.ConsumerType">
         <result property="type" column="type_name"/>
         <result property="description" column="description"/>
     </resultMap>
    <sql id="tableName">
        consumer_type
    </sql>
    <select id="selectAllConsumerType" resultMap="consumerType">
        select type_name, description from <include refid="tableName"/>

    </select>
</mapper>